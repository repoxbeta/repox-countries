{
  "name": "repox-countries",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "node build.js",
    "start": "node dist/index.js",
    "watch": "tsx watch src/index.ts",
    "dev": "NODE_ENV=development yarn tsx src/index.ts",
    "crawl:dev": "NODE_ENV=development USER_NAME=$USER_NAME yarn tsx src/crawl/run.ts",
    "prod": "NODE_ENV=production yarn tsx src/index.ts",
    "crawl:prod": "NODE_ENV=production USER_NAME=$USER_NAME yarn tsx src/crawl/run.ts",
    "clean": "rm -rf dist",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [
    "repox",
    "geonames",
    "countries",
    "states",
    "cities",
    "postal-codes",
    "nodejs",
    "npm-package",
    "geolocation",
    "location-data",
    "world-countries",
    "country-info",
    "city-data",
    "state-data",
    "region-data",
    "address-lookup",
    "geodata",
    "world-data",
    "timezone",
    "country-codes",
    "ISO-3166",
    "latitude-longitude",
    "geo-api",
    "geonames-api",
    "world-regions",
    "address-validation",
    "place-data"
  ],
  "author": "repox.beta@gmail.com",
  "type": "module",
  "license": "MIT",
  "description": "Provides up-to-date country, state, city, language, currency, phone code, and postal code data, sourced daily from the GeoNames API. Integrate easily into forms or use for data verification. Updated versions are released when changes occur.",
  "devDependencies": {
    "@types/axios": "^0.14.4",
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "esbuild": "^0.24.0",
    "eslint": "^8.0.0",
    "prettier": "^3.5.3",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "dotenv": "^16.4.7"
  }
}
